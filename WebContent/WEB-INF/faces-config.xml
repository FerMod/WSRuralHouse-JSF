<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <managed-bean>
  <managed-bean-name>login</managed-bean-name>
  <managed-bean-class>com.ruralhousejsf.model.login.LoginBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>control</managed-bean-name>
  <managed-bean-class>com.ruralhousejsf.model.availability.ControlBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>ok</from-outcome>
   <to-view-id>/client_queryav.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/client_queryav.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>setav</from-outcome>
   <to-view-id>/client_setav.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/client_setav.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>queryav</from-outcome>
   <to-view-id>/client_queryav.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
</faces-config>
